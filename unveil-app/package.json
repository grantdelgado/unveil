{
  "name": "unveil-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:ci": "mkdir -p .next && npx next build",
    "bundle:check": "node scripts/check-first-load.js",
    "build:analyze": "ANALYZE=true next build",
    "build:check": "pnpm build && node scripts/performance-monitor.js",
    "build:budget": "node scripts/bundle-budget-checker.js",
    "perf:check": "node scripts/performance-monitor.js",
    "perf:monitor": "npm run build > .next/build-output.txt 2>&1 && node scripts/performance-monitor.js",
    "perf:budget": "npm run build && npm run build:budget",
    "rum:report": "npx tsx scripts/rum-report.ts",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint --fix .",
    "check:flags": "npx tsx scripts/check-temp-flags.ts",
    "check:email": "npx tsx scripts/check-email-optionality.ts",
    "format": "prettier --write .",
    "test": "vitest run --reporter=default",
    "test:core": "vitest run --reporter=dot -t @core",
    "test:unit": "vitest run --reporter=dot",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:messaging-hooks": "vitest run --config vitest.messaging-hooks.config.ts",
    "test:messaging-hooks:watch": "vitest --watch --config vitest.messaging-hooks.config.ts",
    "test:messaging-hooks:coverage": "vitest --coverage --config vitest.messaging-hooks.config.ts",
    "test:ci-gates": "node scripts/test-ci-gates.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:mobile": "node scripts/mobile-test.js",
    "test:lighthouse": "node scripts/lighthouse-audit.js",
    "lhci:collect": "lhci collect",
    "lhci:assert": "lhci assert",
    "lhci:baseline": "npm run build && npm run start & sleep 5 && lhci collect --config=lighthouserc.js && kill %1",
    "test:all": "pnpm test && pnpm test:e2e && pnpm test:mobile && pnpm test:lighthouse",
    "test:rls": "npx tsx scripts/test-rls-policies.ts",
    "test:dev-auth": "npx tsx scripts/test-dev-auth.ts",
    "test:auth-flow": "npx tsx scripts/test-auth-flow.ts",
    "validate:aasa": "node scripts/verify-aasa.mjs",
    "db:test": "supabase db reset --local && npm run dev-setup",
    "setup:sms": "node scripts/setup-sms.js",
    "test-users:create": "npx tsx scripts/test-user-manager.ts create",
    "test-users:scenario": "npx tsx scripts/test-user-manager.ts scenario",
    "test-users:list": "npx tsx scripts/test-user-manager.ts list",
    "test-users:cleanup": "npx tsx scripts/test-user-manager.ts cleanup",
    "test-users:cleanup-all": "npx tsx scripts/test-user-manager.ts cleanup --all",
    "dev-setup": "npx tsx scripts/dev-setup.ts",
    "dev-reset": "npx tsx scripts/dev-setup.ts --reset",
    "dev-demo": "npx tsx scripts/dev-setup.ts --demo",
    "schema:sync": "npx tsx scripts/sync-schema.ts",
    "schema:validate": "npx tsx scripts/sync-schema.ts --validate",
    "schema:watch": "npx tsx scripts/sync-schema.ts --watch",
    "schema:test": "npx tsx scripts/test-schema-awareness.ts",
    "schema:demo": "npx tsx scripts/demo-schema-awareness.ts",
    "script:backfill-user-ids": "npx tsx scripts/backfill-user-ids.ts",
    "test:user-id-trigger": "npx tsx scripts/test-user-id-trigger.ts",
    "validate:sms-schema": "npx tsx scripts/validate-sms-schema.ts",
    "typecheck": "tsc --noEmit",
    "precommit": "pnpm typecheck && pnpm lint --max-warnings=0 && pnpm test && pnpm check:email && pnpm build",
    "e2e": "pnpm exec playwright test --project=chromium-mobile --project=webkit-mobile",
    "ci:full": "pnpm typecheck && pnpm lint --max-warnings=0 && pnpm test && pnpm check:email && pnpm e2e",
    "test:email-smoke": "pnpm exec playwright test tests/e2e/email-optional.smoke.spec.ts --project=chromium-mobile --project=webkit-mobile"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@sentry/nextjs": "^9.42.1",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-query": "^5.80.7",
    "@tanstack/react-query-devtools": "^5.80.7",
    "clsx": "^2.1.1",
    "commander": "^14.0.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.518.0",
    "next": "15.3.4",
    "papaparse": "^5.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^3.3.0",
    "web-vitals": "^5.0.3",
    "xlsx": "^0.18.5",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.3",
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^14.1.3",
    "@playwright/test": "^1.49.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^20",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^2.1.8",
    "chrome-launcher": "^1.2.0",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-prettier": "^10.1.5",
    "glob": "^11.0.3",
    "jsdom": "^25.0.1",
    "lighthouse": "^12.6.1",
    "markdownlint-cli": "^0.45.0",
    "msw": "^2.6.6",
    "prettier": "^3.5.3",
    "puppeteer": "^24.16.2",
    "supabase": "^1.207.7",
    "tailwindcss": "^4",
    "twilio": "^5.7.0",
    "typescript": "^5",
    "vitest": "^2.1.8"
  }
}
